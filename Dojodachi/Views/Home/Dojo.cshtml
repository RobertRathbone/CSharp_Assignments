@using Dojodachi.Models
@model Dojodachi.Models.Dachi
<!DOCTYPE html>
<html>
    <head>
        <meta charset='utf-8'>
        <title>Dojodachi</title>
        <link rel="stylesheet" href="~/css/style.css"/>
    </head>
<body>
@{
<p> Fullness: @Context.Session.GetInt32("Fullness") Happiness: @Context.Session.GetInt32("Happiness") 
Energy: @Context.Session.GetInt32("Energy") Meals: @Context.Session.GetInt32("Meals") </p>
}
<div class = "container">

   <div class = "jumbotron">

      
      <p>
         @ViewBag.Jim
      </p>
   </div>
   
</div>
    @* <p>Learn about @Context.Session.GetString("findCode")</p> *@
    <form action = "feed" method = "get">
    <button type="submit" class="btn btn-outline-secondary">Feed</button>
    </form>
    <form action = "play" method = "get">
    <button type="submit" class="btn btn-outline-secondary">Play</button>
    </form>
    <form action = "work" method = "get">
    <button type="submit" class="btn btn-outline-secondary">Work</button>
    </form>
    <form action = "sleep" method = "get">
    <button type="submit" class="btn btn-outline-secondary">Sleep</button>
    </form>

</div>
</body>
</html>